    <Script type="xscript5.1"><![CDATA[this.pos2032pu_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	// 부모가 던져준 ID를 변수에 담기
    var sTableId = this.getOwnerFrame().pvTableId; 
    var dsCart = nexacro.getApplication().gdsCart;

    // 전달받은 ID로만 필터링
    dsCart.filter("TABLE_ID == '" + sTableId + "'");

    // 합계 금액 등을 화면에 표시
    var nTotal = dsCart.getSum("TOTAL_PRICE");
    //this.staTotal.set_text(nTotal.toLocaleString() + "원");

    this.staTableName.text=sTableId + "번 테이블";
};
]]></Script>
